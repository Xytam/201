# Inserts a given number into a list so it is still ordered
notInserted='true'
i=0
myList=[1,9,16,24]
num=int(input("Enter a number: "))
while(notInserted=='true'):
    if(num>myList[i]):
        i+=1
        pass
    else:
        myList.insert(i,num)
        notInserted='false'
print(myList)
